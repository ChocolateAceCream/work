<h1>Territory Number: <%= @territory.number %></h1>>

<table class="table table-striped">
  <tr>
    <th>Sales Rep Name</th>
    <th>number</th>
    <th>address</th>
    <th colspan="3"></th>
  </tr>


 <% @salesreps.each do |salesrep| %>
    <tr>
        <td><%= salesrep.sales_rep_number %></td>
        <td><%= salesrep.sales_rep_name %></td>
        <td><%= salesrep.address %></td>
        <td><%= link_to 'Show', display_salesrep_path(salesrep) %></td>
    </tr>
  <% end %>
</table>
 



<%= link_to 'Back', territories_path %>
